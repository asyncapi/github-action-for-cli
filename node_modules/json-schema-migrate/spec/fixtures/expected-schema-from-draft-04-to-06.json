{
  "$id": "my-schema",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "properties": {
    "exclusiveLimits": {
      "exclusiveMinimum": 1,
      "exclusiveMaximum": 3
    },
    "nonExclusiveLimits": {
      "minimum": 1,
      "maximum": 3
    },
    "singleValue": { "const": "foo" },
    "multipleValues": { "enum": ["foo", "bar"] },
    "alwaysValid": true,
    "alwaysInvalid": false
  }
}
